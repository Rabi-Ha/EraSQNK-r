				;V 색상
				MC:LCOUNT2:LCOUNT = COLOR
			ENDIF
		NEXT
	NEXT
	;W
	IF MARK:ARG:정액각인 >= 1
		FOR LCOUNT,13,19
			SUBSTRINGU TT:12:3, LCOUNT, 1
			IF (ENCODETOUNI(RESULTS:0, 0) != 0x0020)
				MM:3:LCOUNT = %RESULTS:0%
				;W 색상
				MC:3:LCOUNT = COLOR
			ENDIF
		NEXT
		FOR LCOUNT,12,20
			SUBSTRINGU TT:12:4, LCOUNT, 1
			IF (ENCODETOUNI(RESULTS:0, 0) != 0x0020)
				MM:4:LCOUNT = %RESULTS:0%
				;W 색상
				MC:4:LCOUNT = COLOR
			ENDIF
		NEXT
	ENDIF
	IF MARK:ARG:정액각인 >= 3
		FOR LCOUNT2,1,SIZE_V-1
			FOR LCOUNT,1,SIZE_H-1
				SUBSTRINGU TT:12:LCOUNT2, LCOUNT, 1
				IF (ENCODETOUNI(RESULTS:0, 0) != 0x0020)
					MM:LCOUNT2:LCOUNT = %RESULTS:0%
					;W 색상
					MC:LCOUNT2:LCOUNT = COLOR
				ENDIF
			NEXT
		NEXT
	ENDIF
	IF MARK:ARG:정액각인 >= 2
		MM:2:16 = %UNICODE(0x0000)%
		MM:2:15 = ♉
		MC:2:15 = COLOR
		MM:3:16 = %UNICODE(0x0000)%
		MM:3:15 = ♈
		MC:3:15 = COLOR
	ENDIF
ENDIF

;음문 출력 
IF 1
	SETFONT "MS Gothic"
	FOR LCOUNT,0,SIZE_V
		PRINTFORM 
		FOR LCOUNT2,0,SIZE_H
				SETCOLOR MC:LCOUNT:LCOUNT2
				PRINTFORM %MM:LCOUNT:LCOUNT2%
		NEXT
		PRINTFORML 
	NEXT
	RESETCOLOR	
	SETFONT
ENDIF

;각인레벨 4 아닐시 종료
IF MARK:ARG:정액각인 != 4
	RETURN 1
ENDIF

;애니메이션 루프 제어부================================================================================================================

;애니메이션 출력속도(1초 : 1000)
;ANI_SPEED
;애니메이션 삽입도달 깊이
;ANI_DIP
;애니메이션 반복횟수
;ANI_RE

;페니스 삽입 있을시만 애니메이션 동작

IF 조교중 && P_TYPE>0 && ( INSERT("인서트상대", ARG) || (TEQUIP:ARG:바이브 && TEQUIP:ARG:촉수) || SELECTCOM == 304)

	;스트로크 애니메이션
	IF P_DIP < ANI_DIP
		P_DIP++
		TWAIT ANI_SPEED, 0
		CLEARLINE 13
		GOTO INPUT_LOOP
	ENDIF
	;스트로크 애니메이션 횟수
	IF ANI_RE > 1
		ANI_RE--
		;삽입 출발지점(초기 삽입시는 처음부터, 두번째부턴 왕복운동)
		IF (ANI_DIP<P_DIP2)
			P_DIP = 1
		ELSE
			P_DIP = ANI_DIP-P_DIP2
		ENDIF
		;사정시 대기시간 길게
		IF (TFLAG:9>0 && SELECTCOM == 304) || (TFLAG:15 && TEQUIP:ARG:촉수!=0) || (COND("前回절정", INSERT("인서트상대", ARG)) && SELECTCOM!=304 && TEQUIP:ARG:촉수==0)
			TWAIT ANI_SPEED*20, 0
		ELSE
			TWAIT ANI_SPEED*8, 0
		ENDIF
		CLEARLINE 13
		GOTO INPUT_LOOP
	ENDIF
	;애니메이션 종료후 대기시간
	TWAIT 1500, 0
ENDIF

;음문 코드 끝================================================================================================================
RETURN 1


