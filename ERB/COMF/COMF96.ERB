SIF CHECK_COM("Ａ자극손훑기") == 96
	CALL SOURCE_COM90, 50

VARSET LOCAL

LOCAL:20 += 10
LOCAL:21 += 100

LOCAL:8 = COM_ORDER_STAIN("Ａ", "Ｍ")*100 + 100
LOCAL:11 = CALCF("실행치による욕정추가")
LOCAL:14 = 3000

;汚れデータを元に, LOCAL:8は上で決定

;ABL:봉사정신をみる
SELECTCASE ABL:봉사정신
CASE 0
	LOCAL:4 = 250
	LOCAL:5 = 50
CASE 1
	LOCAL:4 = 300
	LOCAL:5 = 100
CASE 2
	LOCAL:4 = 350
	LOCAL:5 = 200
CASE 3
	LOCAL:4 = 400
	LOCAL:5 = 300
CASE 4
	LOCAL:4 = 450
	LOCAL:5 = 500
