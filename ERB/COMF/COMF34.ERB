@COM34
;클리캡
IF TEQUIP:촉수
	PRINTL 촉수클리자극
ELSE
	PRINTL 클리캡
ENDIF

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

IF TEQUIP:촉수
	STR:0 = 촉수클리자극
ELSE
	STR:0 = 클리캡
ENDIF

RETURN 1

@FLAG_COM34, ARG

@SOURCE_COM34, ARG
VARSET LOCAL

LOCAL:20 += 10
LOCAL:21 += 20

LOCAL:14 = 300

LOCAL:0 = CALCF("Ｃ刺激", 0, 40)

SIF TALENT:클리피어스
	TIMES LOCAL:0, 2.00

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@EQUIP_COM14
;클리캡装備中
CALL SOURCE_COM34, 100
CALL FLAG_COM34, 100

RETURN 1

@TRAIN_MESSAGE_COM34
;클리캡の着脱
IF TEQUIP:클리캡
	CALL COM_ABLE35
	IF RESULT && TEQUIP:니플캡
		PRINTFORML 니플캡도 뗍니까？
		PRINTFORML [ 0] 예
		PRINTFORML [ 1] 아니오
		CALL INPUT_SELECT, 2
		IF RESULT == 0
			PRINTFORMW %CALLNAME:TARGET%에게서 클리캡과 니플캡을 떼어냈다…
			TEQUIP:클리캡 = 0
			TEQUIP:니플캡 = 0
			RETURN 1
		ENDIF
	ENDIF
	PRINTFORMW %CALLNAME:TARGET%의 클리토리스에서 클리캡을 떼어냈다…
	TEQUIP:클리캡 = 0
ELSE
	CALL COM_ABLE35
	IF RESULT && TEQUIP:니플캡 == 0
		PRINTFORML 니플캡도 답니까？
		PRINTFORML [ 0] 예
		PRINTFORML [ 1] 아니오
		CALL INPUT_SELECT, 2
		IF RESULT == 0
			PRINTFORMW %CALLNAME:TARGET%에게 클리캡과 니플캡을 달아주었다
			PRINTFORMW 두종류의 음구는 %CALLNAME:TARGET%의 민감한 장소를 집요하게 자극해 쾌락을 주고 있다……
			TEQUIP:클리캡 = 1
			TEQUIP:니플캡 = 1
			RETURN 1
		ENDIF
	ENDIF
	PRINTFORMW %CALLNAME:TARGET%의 클리토리스에 잘게 진동하는 캡을 달아주었다
	PRINTFORMW 둥근 모양의 스포이드 같은 그것은 음핵을 집요하게 빨아올려, %CALLNAME:TARGET%에게 달콤한 쾌락을 주고 있다…
	TEQUIP:클리캡 = 1
ENDIF

@TRAIN_MESSAGE_COM203
IF TEQUIP:클리캡
	PRINTFORMW 촉수는 %CALLNAME:TARGET%의 클리토리스를 자극하는 것을 그만두었다…
	TEQUIP:클리캡 = 0
ELSE
	IF RAND:3 == 0
		PRINTFORMW 세밀하게 갈라진 촉수가 %CALLNAME:TARGET%의 클리토리스를 괴롭히기 시작했다…
	ELSEIF RAND:2 == 0
		PRINTFORMW 끈 모양의 촉수가  %CALLNAME:TARGET%의 클리토리스를 휘감고, 고속으로 문지르기 시작했다…
	ELSE
		PRINTFORMW 빨판 모양의 촉수가 %CALLNAME:TARGET%의 클리토리스에 달라붙기 시작했다…
	ENDIF
	TEQUIP:클리캡 = 1
	CALL ADD_STAIN, TARGET, "Ｖ", "촉수"
ENDIF

@EQUIP_COM14_2
;클리캡装備中
IF TEQUIP:촉수
	PRINTL ＜촉수 클리 자극중＞
ELSE
	PRINTL ＜클리캡 장착중＞
ENDIF

RETURN 1
