@COM30
;로터

PRINTL 로터

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:0 = 로터

RETURN 1

@FLAG_COM30, ARG

@SOURCE_COM30, ARG
VARSET LOCAL

LOCAL:20 += 10
LOCAL:21 += 80

LOCAL:12 = 120
LOCAL:14 = 70

;ABL:Ｃ감각をみる
LOCAL:0 = CALCF("Ｃ刺激", 0)

;보지が노출していないと쾌Ｃ減
IF !CHECK_CLO("Ｖ노출") && COND("조교中")
	;바지等をはいていると쾌Ｃ激減
	IF CHECK_OUTER("바지") || CHECK_PANTIES("가죽")
		TIMES LOCAL:0 , 0.20
		TIMES LOCAL:12 , 0.20
	ELSE
		TIMES LOCAL:0 , 0.60
		TIMES LOCAL:12 , 0.50
	ENDIF
ENDIF

CALL CALC, "조교소스", LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12, LOCAL:13, LOCAL:14, LOCAL:15, LOCAL:16, LOCAL:17, LOCAL:18, LOCAL:19, LOCAL:20, LOCAL:21, ARG


@TRAIN_MESSAGE_COM30
PRINTFORM %조사처리(CALLNAME:PLAYER,"는")% 
;인서트中
IF INSERT("합체中")
	PRINTFORM %TEXTS("합체中", PLAYER)% 
ELSEIF !CHECK_CLO("Ｖ노출")
	PRINTFORM %CLO("가랑이")%의 위에서, 
;ELSE
;	SIF ABL:Ｃ감각 >= 3 && (MARK:굴복각인 == 3 || TALENT:연모)
;		PRINT 다리를벌리고 기다리고 있는 
ENDIF
PRINTFORM %CALLNAME:TARGET%의 
IF PENIS(TARGET)
	IF TALENT:음핵 && PALAM:욕정 > 8000
		PRINT 비대해진 
	ELSEIF PALAM:쾌Ｃ > 3000
		PRINT 발기한 
	ENDIF
	PRINT 페니스
ELSE
	IF CALCF("Ｃ발정") >= 3 && RAND:2
		PRINT 비대해진 
	ELSEIF CHECK_CLO("Ｖ노출") && TALENT:클리피어스 && RAND:2
		PRINT 피어스로 장식된 
	ELSEIF CALCF("Ｃ발정") >= 2 && RAND:2
		PRINT 뾰족하게 솟아오른 
	ELSEIF CHECK_CLO("Ｖ노출") && COND("음모") >= 6 && RAND:2
		PRINT 음모에 감춰진 
	ELSEIF CALCF("Ｃ발정") >= 2 && RAND:2
		PRINT 볼록 부풀어오른 
	ELSEIF TALENT:작은몸집
		PRINT 작은 
	ENDIF
	PRINT 클리토리스 
ENDIF
